{
    "name": "rock-solid",
    "version": 2,
    "alias": [
        "rock-solid.now.sh"
    ],
    "builds": [
        {
        "src": "web/package.json",
        "use": "@now/static-build",
        "config": { "distDir": "public" }
        }
    ],
    "routes": [
        {
        "src": "/(.*)", "dest": "/web/$1"
        }
    ]
}
